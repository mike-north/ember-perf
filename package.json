{
  "name": "ember-perf",
  "version": "0.2.0",
  "description": "Performance instrumentation for ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "publish:patch": "npm whoami && npm version patch && git push origin --tags && npm publish",
    "publish:minor": "npm whoami && npm version minor && git push origin --tags && npm publish"
  },
  "repository": "https://github.com/mike-north/ember-perf.git",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "homepage": "https://github.com/mike-north/ember-perf",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "5.6.0",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-pretender": "4.0.0",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "3.0.1",
    "ember-resolver": "13.0.2",
    "ember-source": "5.7.0",
    "ember-source-channel-url": "1.2.0",
    "ember-try": "1.4.0",
    "eslint-plugin-ember": "12.2.1",
    "eslint-plugin-node": "11.1.0",
    "loader.js": "4.7.0",
    "rsvp": "4.8.5"
  },
  "keywords": [
    "ember-addon",
    "performance",
    "instrumentation",
    "load",
    "analytics",
    "perf",
    "metric",
    "render",
    "measure"
  ],
  "dependencies": {
    "ember-cli-babel": "^8.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
