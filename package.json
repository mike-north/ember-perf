{
  "name": "ember-perf",
  "version": "0.2.0",
  "description": "Performance instrumentation for ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "publish:patch": "npm whoami && npm version patch && git push origin --tags && npm publish",
    "publish:minor": "npm whoami && npm version minor && git push origin --tags && npm publish"
  },
  "repository": "https://github.com/mike-north/ember-perf.git",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "homepage": "https://github.com/mike-north/ember-perf",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "2.6.0",
    "ember-cli": "3.5.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-dependency-checker": "2.1.0",
    "ember-cli-eslint": "5.0.0",
    "ember-cli-htmlbars": "2.0.3",
    "ember-cli-htmlbars-inline-precompile": "1.0.2",
    "ember-cli-inject-live-reload": "1.7.0",
    "ember-cli-pretender": "^3.0.0",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "2.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-resolver": "5.0.1",
    "ember-source": "2.18.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "5.0.3",
    "eslint-plugin-node": "8.0.0",
    "loader.js": "4.6.0",
    "rsvp": "^4.7.0"
  },
  "keywords": [
    "ember-addon",
    "performance",
    "instrumentation",
    "load",
    "analytics",
    "perf",
    "metric",
    "render",
    "measure"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
